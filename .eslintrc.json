{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb",
        "airbnb-typescript",
        "prettier"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module",
        "project": "./tsconfig.json"
    },
    "plugins": [
        "react"
    ],
    "ignorePatterns":["vite.config.ts"],
    "rules": {
        "func-style": ["error", "declaration"],
        "react/react-in-jsx-scope": "off",
        "react/require-default-props": "off"
    }
}
